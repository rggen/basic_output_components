<%= module_name %> #(
<% if bit_field.type != :rs %>
  .SET_VALUE      (<%= set_value %>),
  .WRITE_ONLY     (<%= write_only %>),
<% end %>
  .WIDTH          (<%= width %>),
  .INITIAL_VALUE  (<%= initial_value %>)
) u_bit_field (
  .i_clk        (<%= clock %>),
  .i_rst_n      (<%= reset %>),
  .bit_field_if (<%= bit_field_if %>),
  .i_clear      (<%= clear[loop_variables] %>),
  .o_value      (<%= value_out[loop_variables] %>)
);
